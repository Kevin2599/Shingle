<?xml version='1.0' encoding='utf-8'?>
<shingle_options>
  <initialisation_name>
    <string_value lines="1">Antarctica_main_landmass</string_value>
    <comment>-r 'latitude &lt;= -60.0'
-p 1
-dx 10
-exclude_ice_shelves
-bl -  60</comment>
  </initialisation_name>
  <domain_type>
    <string_value lines="1">ocean_ice_shelf</string_value>
  </domain_type>
  <global_parameters/>
  <dataset name="RTopo">
    <form name="Raster">
      <source file_name="../../dataset/RTopo105b_50S.nc" name="Local_file"/>
    </form>
    <projection name="Automatic"/>
  </dataset>
  <surface_geoid_representation name="Antarctica_main_landmass">
    <id>
      <integer_value rank="0">9</integer_value>
    </id>
    <brep_component name="Antarctica_main_landmass_brep">
      <form name="Raster">
        <source name="RTopo"/>
        <region>latitude &lt;= -60.0</region>
        <contourtype name="iceshelfcavity"/>
        <boundary>1</boundary>
      </form>
      <id>
        <integer_value rank="0">3</integer_value>
      </id>
      <representation_type name="BSplines"/>
      <spacing>
        <real_value rank="0">10.0</real_value>
      </spacing>
    </brep_component>
    <closure>
      <bounding_latitude>
        <real_value rank="0">-60.0</real_value>
      </bounding_latitude>
    </closure>
  </surface_geoid_representation>
  <verification>
    <test file_name="data/Antarctica_main_landmass_valid.geo" name="BrepDescription"/>
  </verification>
</shingle_options>
