<?xml version='1.0' encoding='utf-8'?>
<shingle_options>
  <initialisation_name>
    <string_value lines="1">filcher-ronne-shml</string_value>
    <comment>-f shorelines.geo
-n ../dataset/RTopo105b_50S.nc
-no
-b -85.0:-20.0,-89.0:-75.0 -64.0:-30.0,-89.0:-70.0 -30.0:-20.0,-89.0:-75.0
-lat '-65.0'</comment>
  </initialisation_name>
  <domain_type>
    <string_value lines="1">ocean_ice_shelf</string_value>
  </domain_type>
  <global_parameters/>
  <dataset name="RTopo">
    <source name="raster">
      <source file_name="../../dataset/RTopo105b_50S.nc"/>
    </source>
    <projection name="Automatic"/>
  </dataset>
  <surface_geoid_representation name="filchner_ronne_ice_ocean">
    <output file_name="./shorelines.geo"/>
    <brep_component name="filchner_ronne_ice_ocean">
      <source name="RTopo"/>
      <representation_type name="BSplines"/>
    </brep_component>
  </surface_geoid_representation>
  <verification>
    <brep_description file_name="./valid/filchner-ronne-legacy" name="brep_description"/>
  </verification>
</shingle_options>
